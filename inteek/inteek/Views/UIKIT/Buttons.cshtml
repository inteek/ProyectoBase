@{
    ViewBag.Title = "Buttons";
}

<!-- ui buttons -->
<div class="page page-ui-buttons">

    <ol class="breadcrumb breadcrumb-small">
        <li>UI Kit</li>
        <li class="active"><a href="ui.buttons.html">Buttons</a></li>
    </ol>


    <div class="page-wrap">

        <!-- row -->
        <div class="row">
            <!-- color style -->
            <div class="col-sm-12 col-md-4">
                <div class="panel panel-default panel-hovered panel-stacked mb20">
                    <div class="panel-heading">Colors</div>
                    <div class="panel-body">
                        <p class="small">Standard button colors can be created with <code>btn-*</code> classes.</p>
                        <button type="button" class="btn btn-default">Default</button>
                        <button type="button" class="btn btn-primary">Primary</button>
                        <button type="button" class="btn btn-success">Success</button>
                        <button type="button" class="btn btn-info">Info</button>
                        <button type="button" class="btn btn-warning">Warning</button>
                        <button type="button" class="btn btn-danger">Danger</button>
                        <button type="button" class="btn btn-pink">Pink</button>
                        <button type="button" class="btn btn-purple">Purple</button>
                        <button type="button" class="btn btn-default" disabled>Disabled</button>
                    </div>
                </div>
            </div> <!-- #end color style -->
            <!-- Sizes -->
            <div class="col-md-4 col-sm-12">
                <div class="panel panel-default panel-hovered panel-stacked mb20">
                    <div class="panel-heading">Sizes</div>
                    <div class="panel-body">
                        <p class="small">Sizes can be useful for different resolution and you can add them with just <code>btn-xs</code>, <code>btn-sm</code> or <code>btn-lg</code> classes.</p>
                        <button type="button" class="btn btn-default btn-xs">Mini Button</button>
                        <button type="button" class="btn btn-default btn-sm">Small Button</button>
                        <button type="button" class="btn btn-default">Normal Button</button>
                        <button type="button" class="btn btn-default btn-lg">Large Button</button>
                    </div>
                </div>
            </div>	<!-- #end Sizes -->
            <!-- Icon Buttons -->
            <div class="col-md-4 col-sm-12">
                <div class="panel panel-default panel-hovered panel-stacked mb20">
                    <div class="panel-heading">Icons</div>
                    <div class="panel-body">
                        <p class="small">Create circle btns with <code>btn-icon-circle</code> class and flat btns with default class.</p>
                        <button type="button" class="btn btn-twitter ion ion-social-twitter icon"></button>
                        <button type="button" class="btn btn-facebook ion ion-social-facebook icon"></button>
                        <button type="button" class="btn btn-gplus ion ion-social-googleplus icon"></button>
                        <button type="button" class="btn btn-github ion ion-social-github icon"></button>
                        <button type="button" class="btn btn-dribbble ion ion-social-dribbble icon"></button>
                        <button type="button" class="btn btn-youtube ion ion-social-youtube icon"></button>
                        <button type="button" class="btn btn-dribbble ion ion-social-dribbble icon btn-icon-circle"></button>
                        <button type="button" class="btn btn-twitter ion ion-social-twitter btn-icon-circle icon"></button>
                        <button type="button" class="btn btn-facebook ion ion-social-facebook btn-icon-circle icon"></button>
                        <button type="button" class="btn btn-gplus ion ion-social-googleplus btn-icon-circle icon"></button>
                        <button type="button" class="btn btn-github ion ion-social-github btn-icon-circle icon"></button>
                        <button type="button" class="btn btn-youtube ion ion-social-youtube btn-icon-circle icon"></button>
                    </div>
                </div>
            </div>
            <!-- #Icon Buttons -->

        </div> <!-- #end row -->
        <!-- row -->
        <div class="row">

            <div class="col-sm-12">
                <!-- Button Groups -->
                <div class="panel panel-default panel-hovered panel-stacked mb20">
                    <div class="panel-heading">Button Groups</div>
                    <div class="panel-body">
                        <div class="row">
                            <div class="col-sm-4">
                                <p class="small text-warning text-uppercase text-normal">Default Group</p>
                                <p class="small mb15">Multiple buttons wrapped inside <code>btn-group</code> class.</p>
                                <div class="btn-group">
                                    <button type="button" class="btn btn-default ion ion-clock"></button>
                                    <button type="button" class="btn btn-default ion ion-compass"></button>
                                    <button type="button" class="btn btn-default ion ion-pinpoint"></button>
                                </div>
                                <br>
                                <div class="btn-group">
                                    <button type="button" class="btn btn-default btn-sm btn-pink ion ion-arrow-graph-up-right"></button>
                                    <button type="button" class="btn btn-default btn-sm btn-pink ion ion-arrow-graph-down-right"></button>
                                    <button type="button" class="btn btn-default btn-sm btn-pink ion ion-arrow-graph-up-left"></button>
                                </div>
                            </div>	<!-- #end inner col -->

                            <div class="col-sm-4">
                                <p class="small text-warning text-uppercase text-normal">Button Toolbar</p>
                                <p class="small mb15">Use <code>btn-toolbar</code> wrapped around multiple <code>btn-group</code> classes.</p>
                                <div class="btn-toolbar">
                                    <div class="btn-group">
                                        <button type="button" class="btn btn-default ion ion-log-in"></button>
                                        <button type="button" class="btn btn-default ion ion-log-out"></button>
                                    </div>
                                    <div class="btn-group">
                                        <button type="button" class="btn btn-default ion ion-arrow-shrink"></button>
                                        <button type="button" class="btn btn-default ion ion-arrow-expand"></button>
                                        <button type="button" class="btn btn-default ion ion-arrow-move"></button>
                                    </div>
                                    <div class="btn-group">
                                        <button type="button" class="btn btn-default ion ion-paperclip"></button>
                                    </div>
                                </div>
                            </div> <!-- #end inner col -->

                            <div class="col-sm-4">
                                <p class="small text-warning text-uppercase text-normal">Vertical Group</p>
                                <p class="small mb15">Use <code>btn-group-vertical</code> to arrange buttons vertically.</p>
                                <div class="btn-group-vertical">
                                    <button type="button" class="btn btn-default ion ion-gear-a"></button>
                                    <div class="btn-group dropdown">
                                        <button class="btn btn-pink dropdown-toggle ion ion-help-buoy" data-toggle="dropdown">&nbsp;<span class="caret"></span></button>
                                        <ul class="dropdown-menu">
                                            <li><a href="j:;">Action</a></li>
                                            <li><a href="j:;">Another action</a></li>
                                            <li><a href="j:;">Something else here</a></li>
                                            <li class="divider"></li>
                                            <li><a href="j:;">Separated link</a></li>
                                        </ul>
                                    </div>
                                    <button type="button" class="btn btn-default ion ion-trash-a"></button>
                                </div> <!-- #end btn-group-vertical -->
                            </div> <!-- #end inner col -->
                        </div>
                    </div> <!-- #end panel-body -->
                </div> <!-- #end panel -->
            </div> <!-- #end col -->

        </div> <!-- #end row -->
        <!-- row -->
        <div class="row">

            <!-- button variations -->
            <div class="col-md-7 col-sm-12">
                <div class="panel panel-default panel-hovered panel-stacked mb20">
                    <div class="panel-heading">Variations</div>
                    <div class="panel-body">
                        <button type="button" class="btn btn-line-primary">Lined Primary</button>
                        <button type="button" class="btn btn-pink btn-rounded">Round</button>
                        <button type="button" class="btn btn-line-success btn-rounded">Lined Round</button>
                        <button type="button" class="btn btn-line-info btn-icon-inline"><i class="ion ion-wrench"></i>Settings</button>
                        <button type="button" class="btn btn-danger btn-icon-inline"><i class="ion ion-medkit"></i>Health</button>
                        <button type="button" class="btn btn-purple btn-w140">Fixed Width</button>

                        <button type="button" class="btn btn-default btn-twitter btn-rounded"><i class="ion ion-social-twitter"></i>Twitter</button>
                        <button type="button" class="btn btn-default btn-facebook btn-rounded"><i class="ion ion-social-facebook"></i>Facebook</button>
                        <button type="button" class="btn btn-default btn-gplus"><i class="ion ion-social-googleplus"></i>Google</button>
                        <button type="button" class="btn btn-default btn-dribbble"><i class="ion ion-social-dribbble"></i>Dribbble</button>
                        <button type="button" class="btn btn-default btn-flickr"><i class="ion ion-social-skype"></i>Skype</button>
                    </div>
                </div>
            </div> <!-- #end button variations -->
            <!-- block buttons -->
            <div class="col-md-5 col-sm-6">
                <div class="panel panel-default panel-hovered panel-stacked mb20">
                    <div class="panel-heading">Block Buttons</div>
                    <div class="panel-body">
                        <p class="small">Full size buttons created with <code>btn-block</code> class.</p>
                        <button type="button" class="btn btn-line-default btn-lg btn-block">Block Button Large</button>
                        <button type="button" class="btn btn-primary btn-block">Block Button Normal</button>
                    </div>
                </div>
            </div> <!-- #end block buttons -->

        </div>
        <!-- #end row -->
        <!-- row -->
        <div class="row">
            <!-- button variations -->
            <div class="col-md-12 col-sm-12">
                <div class="panel panel-default panel-hovered panel-stacked mb20">
                    <div class="panel-heading">Button Tags</div>
                    <div class="panel-body">
                        <a class="btn-tag btn-tag-primary" href="javascript:;">CSS3</a>
                        <a class="btn-tag btn-tag-success" href="javascript:;">JavaScript</a>
                        <a class="btn-tag btn-tag-danger" href="javascript:;">PHP</a>
                        <a class="btn-tag btn-tag-info" href="javascript:;">Wordpress</a>
                        <a class="btn-tag btn-tag-warning" href="javascript:;">Photoshop</a>
                        <a class="btn-tag btn-tag-pink" href="javascript:;">Illustrator</a>
                        <a class="btn-tag btn-tag-purple" href="javascript:;">Animation</a>
                        <a class="btn-tag btn-tag-default" href="javascript:;">Books</a>
                    </div>
                </div>
            </div> <!-- #end button variations -->

        </div>
        <!-- #end row -->

    </div> <!-- #end page-wrap -->
</div>
<!-- #end ui buttons -->


<!-- Scripts-->
@Scripts.Render("~/scripts/app.js")

